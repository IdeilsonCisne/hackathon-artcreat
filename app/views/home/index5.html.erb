<form action="">
    <div class="mt-5 mb-5">
        <div class="row">
            <h3 class="mb-5">Atividades Contratas</h3>
            <div class="col">
                <h4>Nível atividade*</h4>
                <div class="dropdown">
                    <button class="btn btn-outline-primary btn-transparent dropdown-toggle btn-block w-100 h-75" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                      Selecione o nível de técnica
                    </button>
                </div>
            </div>

            <div class="col">
                <h4>Atividade Profissional</h4>
                <div class="dropdown">
                    <button class="btn btn-outline-primary btn-transparent dropdown-toggle btn-block w-100 h-75" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                      Selecione um tipop de atividade profissional
                    </button>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col">
                <h4>Atividade(s)*</h4>
                <div class="dropdown">
                    <button class="btn btn-outline-primary btn-transparent dropdown-toggle btn-block w-100 h-75" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                      Selecione o código ou nome da atividade
                    </button>
                    10.3.4.5 - geodésico <br>
                    10.3.4.5 - geodésicos <br>
                    10.3.4.6 - topográficos <br>
                    10.3.4.7 - pelo sol <br>
                    10.3.4.8 - urbano
                </div>
            </div>

            <div class="col">
                <div class="row">
                    <div class="col">
                        <h4>Unidade de medida*</h4>
                        <button class="btn btn-outline-primary btn-transparent dropdown-toggle btn-block w-100" type="button" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-expanded="false">
                            Selecione a un. medida.
                        </button>
                    </div>
                    <div class="col">
                        <h4>Quantidade*</h4>
                        <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="00,00">
                    </div>
                </div>
            </div>
        </div>
        <hr class="mt-4 mb-4">

        <div class="row">
            <h4>Contratante*</h4>
            <div class="col">
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="Pessoa Física[CPF]">
            </div>
            <div class="col">
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="Pessoa física [NOME]">
            </div>
            <div class="col">
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="Pessoa jurídica [CNPJ]">
            </div>
            <div class="col">
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="Pessoa jurídica [NOME]">
            </div>
        </div>
        <hr class="mt-4 mb-4">

        <div class="row">
            <h4>Proprietário*</h4>
            <div class="col">
                <button class="btn btn-outline-primary btn-transparent btn-block w-100" type="button">
                    Aproveitar dados do contratante
                </button>
            </div>
            <div class="col">
                <button class="btn btn-outline-primary btn-transparent  btn-block w-100" type="button">
                    Adicionar dados do contratante
                </button>
            </div>
        </div>
        <hr class="mt-4 mb-4">

        <div class="row">
            <h4>Endereço da obra/serviço</h4>
            <div class="col">
                <button class="btn btn-outline-primary btn-transparent btn-block w-100" type="button">
                    Utilizar dados do contratante
                </button>
            </div>
            <div class="col">
                <button class="btn btn-outline-primary btn-transparent  btn-block w-100" type="button">
                    Utilizar endereço do proprietário
                </button>
            </div>
            <div class="col">
                <button class="btn btn-outline-primary btn-transparent  btn-block w-100" type="button">
                    Adicionar novo endereço
                </button>
            </div>
        </div>
        <hr class="mt-4 mb-4">
        
        <div class="row">
            <h3>Dados do contrato</h3>
            <div class="row">
                <div class="col">
                    <h4>Número do contrato</h4>
                    <input class="btn btn-outline-primary btn-transparent btn-block w-100" placeholder="Digite o número do contrato.">
                </div>
                <div class="col">
                    <h4>Celebrado em</h4>
                    <input class="btn btn-outline-primary btn-transparent btn-block w-100" type="cal" placeholder="">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <h4>Código de obra pública</h4>
                    <input class="btn btn-outline-primary btn-transparent btn-block w-100" type="cal" placeholder="Digite o código da obra pública.">
                </div>
                <div class="col"></div>
            </div>
            </div>
        </div>

        <div class="row">
            <h4>Prazo e valores da obra serviço</h4>
            <div class="col">
                Início da obra/serviço
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" type="cal" placeholder="">
            </div>
            <div class="col">
                Previsão do término
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" type="cal" placeholder="">
            </div>
            <div class="col">
                Valor da obra/serviço (R$)
                <input class="btn btn-outline-primary btn-transparent btn-block w-100" type="cal" placeholder="00,00">
            </div>
        </div>
        <hr class="mt-4 mb-4">

        <div class="row mt-5 mb-5">
            <div class="col text-center">
                <div class="g-grid">
                    <button type="button" class="btn btn-success w-25">Finalizar Cadastro</button>
                </div>
            </div>
        </div>

    </div>
</form>